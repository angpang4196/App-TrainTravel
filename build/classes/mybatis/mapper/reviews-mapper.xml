<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="reviews">
	<select id="orderByWrited" parameterType="string" resultType="data.Review">
		select * from reviews where contentid = #{contentid} order by re_writed
	</select>
	<insert id="save" parameterType="hashmap">
		insert into reviews values(#{name}, #{code})
	</insert>
</mapper>
