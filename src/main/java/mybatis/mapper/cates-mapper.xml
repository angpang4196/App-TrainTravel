<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cates">
	<select id="findByType" resultType="data.cate.Cate" parameterType="string">
		select * from cates where type = #{type}
	</select>
	<select id="findAll" resultType="data.cate.Cate">
		select * from cates 
	</select>
</mapper>
