<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="losts">
	<select id="findBySpell" resultType="data.LostItem" parameterType="string">
		select * from spells where spell = #{spell}
	</select>
	<select id="findAll" resultType="data.LostItem" >
		SELECT * FROM SPELLS
	</select>
</mapper>
